##api req
#macro(apiReqScript)
    #foreach($item in $tableList)
        #if($item.componentType=="selectApi")##selectApi
        let ${item.field}Data = $ref([])
        const ${item.field}Req = () => {
        let reqConfig = {
        url: '${item.api}',
        method: '${item.method}',
        }
        axiosReq(reqConfig).then(({data}) => {
            ${item.field}Data = data.records
        })}
        #end
    #end
#end
##form key
#macro(formKeyScrpt)
    id: '',
    #foreach($item in $tableList)
        $item.field:"",
        #if($qcItem.componentType=="daterange")
            ${item.field}Arr:[],
        #end
    #end
#end
